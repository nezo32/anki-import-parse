{
  "name": "@anki-import-parser/main",
  "version": "1.1.1",
  "description": "Bun parser-importer to ANKI cards",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "author": {
    "name": "nezo"
  },
  "license": "MIT",
  "bin": {
    "anki-import-parser": "dist/index.js"
  },
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "start": "bun src/index.ts",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "prepublish": "npm run build",
    "build": "tsc",
    "build:exe": "bun build ./src/index.ts --compile --outfile anki-pfd-parser",
    "build:all": "npm run build && npm run build:exe"
  },
  "dependencies": {
    "argparse": "^2.0.1",
    "cli-progress": "^3.12.0",
    "dotenv": "^17.2.3",
    "node-html-parser": "^7.0.1",
    "openai": "^6.15.0",
    "pdf2json": "^4.0.0",
    "typescript": "^5.9.3",
    "yanki-connect": "3.2.2"
  },
  "devDependencies": {
    "@types/argparse": "^2.0.17",
    "@types/cli-progress": "^3.11.6",
    "@types/node": "^25.0.3",
    "bun": "latest"
  }
}
